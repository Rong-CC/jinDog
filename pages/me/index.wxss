.personage{
  width: 100%;
  height: 460rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-image:url(https://s1.ax1x.com/2018/10/19/iwH6PI.jpg);
  background-size: 100% 100%;
}
.portrait{
  width: 120rpx;
  height: 120rpx;
  display: inline-block;
  border-radius: 50%;
  overflow: hidden;
}
.username{
  padding-top:18rpx; 
}
.maginTop{
  margin-top: 17rpx;
}
.list{
  height: 90rpx;
  line-height: 90rpx;
  background-color: #fff;
  
  /* display: flex; */
}
.list-box{
  display: flex;
  padding-right: 40rpx;
  border: 1rpx solid #f5f5f5;
  box-sizing: border-box;
  justify-content: space-between;
}
.font{
  padding: 26rpx;
}
.loading{
  padding: 10rpx;
  text-align: center;
}
.loading:before{
  display: inline-block;
  margin-right: 5rpx;
  vertical-align: middle;
  content: '';
  width: 40rpx;
  height: 40rpx;
  background-image: url(https://s1.ax1x.com/2018/10/22/iDnBwT.gif);
  background-size: contain;
  animation: rotate 1s linear infinite;
  transition: All 0.4s ease; 
  -webkit-transition: All 0.4s ease;
}
